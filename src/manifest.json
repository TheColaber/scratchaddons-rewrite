{
  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "2.0.0",
  "description": "The Revamped, Rewritten version of Scratch Addons!",
  "default_locale": "en",
  "background": {
    "service_worker": "background/index.ts",
    "type": "module"
  },
  "action": {
    "default_popup": "webpages/popup/index.html"
  },
  "icons": {
    "16": "images/icon-blue-16.png",
    "32": "images/icon-blue-32.png",
    "1024": "images/icon.png"
  },
  "permissions": ["storage"]
}
